{
  "name": "@fireflysemantics/slice",
  "version": "6.5.0",
  "description": "Lightweight Angular state management",
  "main": "index.ts",
  "scripts": {
    "update": "ncu -a && npm i",
    "clean": "del-cli target && del-cli dist && del-cli doc",
    "test": "jest",
    "doc": "typedoc --out doc --exclude **/*.spec.ts ./src/",
    "compile": "del-cli target && tsc",
    "dist": "npm run compile && del-cli dist && node createdist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fireflysemantics/slice.git"
  },
  "keywords": [
    "Typescript",
    "Angular",
    "State",
    "Query",
    "Reactive",
    "RxJS",
    "Observables",
    "Multicasting"
  ],
  "_moduleAliases": {
    "@fireflysemantics/*": ".target/src/*"
  },
  "author": "Ole Ersoy",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/fireflysemantics/slice/issues"
  },
  "homepage": "https://github.com/fireflysemantics/slice#readme",
  "devDependencies": {
    "@types/jest": "^24.0.11",
    "@types/node": "^11.12.2",
    "cpy": "^7.1.0",
    "del-cli": "^1.1.0",
    "globby": "^9.1.0",
    "jest": "^24.5.0",
    "mkdirp": "^0.5.1",
    "npm-check-updates": "^3.1.3",
    "recursive-copy": "^2.0.10",
    "source-map-support": "^0.5.11",
    "ts-jest": "^24.0.1",
    "ts-node": "^8.0.3",
    "tsconfig-paths": "^3.8.0",
    "typedoc": "^0.14.2",
    "typescript": "^3.4.1"
  },
  "dependencies": {
    "@fireflysemantics/collections": "^1.0.2",
    "@fireflysemantics/is": "^1.8.15",
    "@types/uuid": "^3.4.4",
    "rxjs": "^6.4.0",
    "typescript-logging": "^0.6.3",
    "uuid": "^3.3.2"
  },
  "requiredAngularVersion": ">=6.0.0 <7.0.0",
  "peerDependencies": {
    "rxjs": "^6.0.0"
  }
}
