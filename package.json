{
  "name": "@fireflysemantics/slice",
  "version": "6.3.0",
  "description": "Lightweight Angular state management",
  "main": "index.ts",
  "scripts": {
    "update": "ncu -a && npm i",
    "clean": "del-cli target && del-cli dist && del-cli doc",
    "unit-test": "mocha -r ts-node/register src/**/*.spec.ts",
    "test": "nyc mocha",
    "doc": "typedoc --out doc --exclude **/*.spec.ts ./src/",
    "compile": "del-cli target && tsc",
    "dist": "npm run compile && del-cli dist && node createdist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fireflysemantics/slice.git"
  },
  "keywords": [
    "Typescript",
    "Angular",
    "State",
    "Query",
    "Reactive",
    "RxJS",
    "Observables",
    "Multicasting"
  ],
  "_moduleAliases": {
    "@fireflysemantics/*": ".target/src/*"
  },
  "author": "Ole Ersoy",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/fireflysemantics/slice/issues"
  },
  "homepage": "https://github.com/fireflysemantics/slice#readme",
  "devDependencies": {
    "@types/chai": "^4.1.4",
    "@types/mocha": "^5.2.5",
    "@types/node": "^10.10.1",
    "chai": "^4.1.2",
    "cpy": "^7.0.1",
    "del-cli": "^1.1.0",
    "globby": "^8.0.1",
    "istanbul": "^0.4.5",
    "mkdirp": "^0.5.1",
    "mocha": "^5.2.0",
    "npm-check-updates": "^2.14.2",
    "nyc": "^13.0.1",
    "recursive-copy": "^2.0.9",
    "source-map-support": "^0.5.9",
    "ts-node": "^7.0.1",
    "tsconfig-paths": "^3.6.0",
    "typedoc": "^0.12.0",
    "typescript": "^3.0.3"
  },
  "dependencies": {
    "@fireflysemantics/collections": "^1.0.2",
    "@fireflysemantics/is": "^1.8.0",
    "@types/uuid": "^3.4.4",
    "rxjs": "^6.3.2",
    "typescript-logging": "^0.6.2",
    "uuid": "^3.3.2"
  },
  "requiredAngularVersion": ">=6.0.0 <7.0.0",
  "peerDependencies": {
    "rxjs": "^6.0.0"
  }
}
